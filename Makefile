ROOTDIR = .
include $(ROOTDIR)/config.mk

.PHONY:	code clean

#==============================================================================
# The default build target.
#==============================================================================
all:	code
#==============================================================================

code:
	$(MAKE) -C code

#==============================================================================
# Clean up the targets built by 'make all'.
#==============================================================================
clean:	code_clean
code_clean:
	$(MAKE) -C code clean

