include config.mk

.PHONY:	code clean

#==============================================================================
# The default build target.
#==============================================================================
all:	code
#==============================================================================

code:
	$(MAKE) -C $(CODE_INSTALL_DIR)
#==============================================================================
# Clean up the targets built by 'make all'.
#==============================================================================
clean:	code_clean
code_clean:
	$(MAKE) -C $(CODE_INSTALL_DIR) clean

