<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Swig Convertor"
	>
	<Rules>
		<CustomBuildRule
			Name="Swig Convertor"
			DisplayName="swig"
			CommandLine="swig.exe -c++ -I$(ProjectDir)..\..\..\code\pkg_Core\Interface -python $(InputPath)"
			Outputs="$(IntDir)\$(InputName)._swig"
			FileExtensions="*.i"
			ExecutionDescription="swig generating"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
